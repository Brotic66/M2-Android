<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="brotic.findmyfriends">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Activity.LauncherActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.ConnexionActivity"
            android:label="@string/title_activity_connexion"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.InscriptionActivity"
            android:label="@string/title_activity_inscription"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>
